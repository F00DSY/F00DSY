            local num = 1
            for _,w in pairs(v) do
                w.Name = w.Name..num
                num = num + 1
            end
        end
    end
    hatnameclone = nil
    
    local DeadChar = game:FindFirstChildOfClass("Players").LocalPlayer.Character
    
